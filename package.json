{
    "name": "alt-gpt-v1",
    "version": "1.0.0",
    "src/main": "index.ts",
    "license": "MIT",
    "author": "",
    "scripts": {
        "build": "tsc",
        "serve": "vercel env pull --environment=preview .env && NODE_ENV=preview vercel dev --listen 8080",
        "debug": "nodemon --watch 'src/**/*.ts' --exec tsx --inspect -r dotenv/config node_modules/edge.libx.js/src/helpers/localServer.ts src/api/index.ts",
        "debug:tunnel": "ngrok http http://localhost:8080",
        "format": "prettier --config .prettierrc 'src/**/*.ts' 'tests/**/*.ts' --write",
        "watch": "tsc -w",
        "test": "jest",
        "test:debug": "node --inspect ./node_modules/jest/bin/jest.js --runInBand --colors --verbose",
        "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --coverageReporters=html",
        "bump": "./bump.sh"
    },
    "bin": {
        "cli": "bin/cli.ts"
    },
    "files": [
        "bin/**",
        "build/**",
        "dist/**",
        "src/**",
        "*.js",
        "*.ts"
    ],
    "dependencies": {
        "@ai-sdk/anthropic": "^1.2.12",
        "@ai-sdk/cerebras": "^0.2.14",
        "@ai-sdk/deepseek": "^0.2.14",
        "@ai-sdk/google": "^1.2.19",
        "@ai-sdk/google-vertex": "^2.2.24",
        "@ai-sdk/groq": "^1.2.9",
        "@ai-sdk/mistral": "^1.2.8",
        "@ai-sdk/openai": "^1.3.22",
        "@supabase/ssr": "^0.6.1",
        "@supabase/supabase-js": "^2.50.0",
        "@trpc/client": "^11.3.1",
        "@trpc/server": "^11.3.1",
        "ai": "^4.3.16",
        "dotenv": "^16.5.0",
        "edge.libx.js": "^0.1.5",
        "zod": "^3.25.56"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^22.7.9",
        "jest": "^29.7.0",
        "jest-junit": "^16.0.0",
        "jest-junit-reporter": "^1.1.0",
        "nodemon": "^3.1.10",
        "prettier": "^3.3.3",
        "ts-jest": "^29.2.5",
        "tsx": "^4.19.4",
        "typescript": "^5.6.3"
    },
    "jest-junit": {
        "suiteName": "jest tests",
        "outputDirectory": "build/test_reports",
        "output": "./.tmp/test/TEST-RESULTS.xml",
        "classNameTemplate": "{classname} - {title}",
        "titleTemplate": "{classname} - {title}",
        "ancestorSeparator": " > ",
        "usePathForSuiteName": "true"
    }
}